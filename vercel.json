{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.py" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "installCommand": "cd web && npm install --legacy-peer-deps",
  "buildCommand": "cd web && npm run build",
  "outputDirectory": "web/dist",
  "framework": "vite"
}
